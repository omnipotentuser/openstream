z0()
  z-index 0
z1()
  z-index 100
z2()
  z-index 200
z3()
  z-index 300

#lounge-main
  display none
  h1
    font-size 24px
  .lounge-overlay
    position absolute
    left 0px
    top 60px
    right 0px
    bottom 0px
  .lounge-inline-block
    background-color rgba(100,100,100, 1)
    display inline-block
    position absolute
    left 210px
    top 0px
    right 0
    bottom 0
  #lounge-container-list
    background-color rgba(0,0,0,0.3)
    display inline-block
    height 100%
    width 200px
    float left
  #lounge-container-setup
    width 400px
    height 230px
    background-color rgba(230,235,240,0.8)
    margin 100px auto
  .lounge-form-input
    height 35px
    font-size 24px
    cursor pointer
  .lounge-form-btn
    height 35px
    font-size 18px
    cursor pointer
    background-color rgba(100,100,100,.4)
    border 0px solid
    width 100px
    &:hover
      background-color rgba(100,100,100,.3)
    &:last-of-type
      margin-left 30px
  #lounge-modal-create
    height 200px
    width 370px
    margin 15px 15px
    top 15px
    vertical-align center
    text-align center
    position relative
    background-color rgba(50,50,50,.1)
    display block
  .lounge-boxed
    position relative
    top 10px
    margin 10px 15px
    text-align center
  #lounge-container-session
    display none
